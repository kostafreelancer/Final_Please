<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="lancer.mappers.e_mainMapper">
	
	<select id="countFreelancer" resultType="int">
		select count(*) from FREELANCER
	</select>
	
	<select id="countProject" resultType="int">
		select count(*) from PROJECT
	</select>
	
	<select id="e_developer" resultType="int">
		select count(f_major) from freelancer where f_major = '개발'
	</select>
	
	<select id="e_designer" resultType="int">
		select count(f_major) from freelancer where f_major = '디자인'
	</select>
	
	<select id="e_planner" resultType="int">
		select count(f_major) from freelancer where f_major = '기획'
	</select>
  
</mapper>  










